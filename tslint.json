{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-trailing-whitespace": [
      true,
      "ignore-comments"
    ],
    "only-arrow-functions": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always"
      }
    ]
  },
  "rulesDirectory": []
}